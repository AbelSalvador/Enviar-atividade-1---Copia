#include <stdio.h>
#include <math.h>

int main() {
float raio, altura, volume;
float pi;

printf ("Insira o valor do raio da base e a altura de um cilindro");
scanf ("%f %f", &raio, &altura);
 
pi = 3.14;

volume = pi * pow( raio, 2 ) * altura;

printf ("O volume do cilindro Ã© %.3f\n", volume);

return 0;