#include <stdio.h>

int main() {
    int n1, n2, n3;
    int media;

printf ("insira o primeiro número inteiro");
scanf ("%d", &n1);

printf ("insira o segundo número inteiro");
scanf ("%d", &n2);

printf ("insira o terceiro número inteiro");
scanf ("%d", &n3);

media = (n1 + n2 + n3) / 3;

printf ("a média aritmética dos três números inteiros é igual %d\n", media);


return 0;


}
